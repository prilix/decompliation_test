<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="#ffffffff" />
            <size android:height="30.0dip" android:width="30.0dip" />
            <stroke android:width="1.0dip" android:color="@color/switch_unchecked_color" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="#ffffffff" />
            <size android:height="30.0dip" android:width="30.0dip" />
            <stroke android:width="1.0dip" android:color="@color/switch_checked_color" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="#ffffffff" />
            <size android:height="30.0dip" android:width="30.0dip" />
            <stroke android:width="1.0dip" android:color="@color/switch_unchecked_color" />
        </shape>
    </item>
</selector>