<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="30.0dip" />
            <solid android:color="@color/switch_unchecked_color" />
            <stroke android:width="1.0dip" android:color="@color/zone_list_text" />
            <size android:height="30.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="33.0dip" />
            <solid android:color="@color/switch_checked_color" />
            <size android:height="30.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="30.0dip" />
            <solid android:color="@color/switch_checked_color_disable" />
            <size android:height="33.0dip" />
        </shape>
    </item>
</selector>